{% extends 'base.html.twig' %}

{% block body %}
    <h1>Serviciotipo</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ servicioTipo.id }}</td>
            </tr>
            <tr>
                <th>Servicio</th>
                <td>{{ servicioTipo.servicio }}</td>
            </tr>
            <tr>
                <th>Precio</th>
                <td>{{ servicioTipo.precio }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('serviciotipo_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('serviciotipo_edit', { 'id': servicioTipo.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
