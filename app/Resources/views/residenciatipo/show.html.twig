{% extends 'base.html.twig' %}

{% block body %}
    <h1>Residenciatipo</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ residenciaTipo.id }}</td>
            </tr>
            <tr>
                <th>Residencia</th>
                <td>{{ residenciaTipo.residencia }}</td>
            </tr>
            <tr>
                <th>Abrev</th>
                <td>{{ residenciaTipo.abrev }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('residenciatipo_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('residenciatipo_edit', { 'id': residenciaTipo.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
